<!DOCTYPE html>
<html>
  <head>
    <title>Users</title>
    <link rel='stylesheet' href='/stylesheets/style.css'/>
  </head>
  <body>
  <table border="1">
    <tr style="font-size: 17px; font-weight: bold;">
      <td>no</td>
      <td>id</td>
      <td>pw</td>
      <td>name</td>
      <td>email</td>
      <td>address</td>
    </tr>
    
    <% for(var i = 0; i < users.length; i++) { %>
        <tr>
          <td><%= users[i].no %></td>
          <td><%= users[i].id %></td>
          <td><%= users[i].password %></td>
          <td><%= users[i].name %></td>
          <td><%= users[i].email %></td>
          <td><%= users[i].address %></td>
        </tr>
    <% } %>

  </table>
  </body>
</html>
